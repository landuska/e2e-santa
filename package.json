{
  "name": "e2e-santa",
  "version": "1.0.0",
  "description": "",
  "main": "cypress.config.js",
  "scripts": {
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:santa": "npm run cy:run -- --spec 'cypress/e2e/santa-box-create.cy.jss'",
    "cy:cloud": "cypress run --record --key 40b31b68-6a0e-47c8-a287-f6460d41990d",
    "cy:spec2": "cypress run -- --spec 'cypress/e2e/test2/creatingBox2.feature' --record --key 40b31b68-6a0e-47c8-a287-f6460d41990d",
    "cy:spec1": "cypress run -- --spec 'cypress/e2e/test/creatingBox.feature' --record --key 40b31b68-6a0e-47c8-a287-f6460d41990d",
    "cy:nokey:spec2": "cypress run --spec 'cypress/e2e/test2/creatingBox2.feature'",
    "cy:nokey:spec1": "cypress run --spec 'cypress/e2e/test/creatingBox.feature' --env allure=true",
    "cy:nokey:laptop": "cypress run --config-file cypress.config.js --spec 'cypress/e2e/test/creatingBox.feature'",
    "cy:nokey:monitor": "cypress run --config-file cypress.config2.js --spec 'cypress/e2e/test2/creatingBox2.feature'",
    "cy:laptop": "cypress run --config-file cypress.config.js --spec cypress/e2e/test/creatingBox.feature --record --key 40b31b68-6a0e-47c8-a287-f6460d41990d --group laptop --ci-build-id ${BUILD_TAG}",
    "cy:monitor": "cypress run --config-file cypress.config2.js --spec cypress/e2e/test2/creatingBox2.feature --record --key 40b31b68-6a0e-47c8-a287-f6460d41990d --group monitors --ci-build-id ${BUILD_TAG}",
    "cy:parallel": "npm-run-all --parallel cy:laptop cy:monitor",
    "cy:allure:report": "cypress allure generate allure-results --clean -o allure-report",
    "cy:allure-open":"cypress allure open allure-report"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@faker-js/faker": "^7.6.0",
    "cypress": "^12.0.0"
  },
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@shelex/cypress-allure-plugin": "^2.40.1",
    "allure-commandline": "^2.27.0",
    "allure-cucumberjs": "^2.12.0",
    "concurrently": "^8.2.2",
    "npm-run-all": "^4.1.5"
  }
}
